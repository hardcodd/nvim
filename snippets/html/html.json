{
  "Site Header": {
    "prefix": "header",
    "body": "<header class=\"header\" id=\"header\">\n\t<div class=\"container\">\n\t\t<div class=\"header__content\">\n\t\t\t$1\n\t\t</div>\n\t</div>\n</header>"
  },
  "Site Footer": {
    "prefix": "footer",
    "body": "<footer class=\"footer\" id=\"footer\">\n\t<div class=\"container\">\n\t\t<div class=\"footer__content\">\n\t\t\t$1\n\t\t</div>\n\t</div>\n</footer>"
  },
  "Sidebar": {
    "prefix": "sidebar",
    "body": "<aside class=\"sidebar\" id=\"sidebar\">\n\t<div class=\"sidebar__content\">\n\t\t$1\n\t</div>\n</aside>"
  },
  "Main": {
    "prefix": "main",
    "body": "<main class=\"main\" id=\"main\">\n\t$1\n</main>"
  },
  "Container": {
    "prefix": "cnt",
    "body": "<div class=\"container\">\n\t$1\n</div>"
  },
  "Container fluid": {
    "prefix": "cntf",
    "body": "<div class=\"container fluid\">\n\t$1\n</div>"
  },
  "Container left": {
    "prefix": "cntl",
    "body": "<div class=\"container left\">\n\t$1\n</div>"
  },
  "Container right": {
    "prefix": "cntr",
    "body": "<div class=\"container right\">\n\t$1\n</div>"
  },
  "Locals": {
    "prefix": "locals",
    "body": "<script locals>\n\tmodule.exports = {\n\t\t$1\n\t}\n</script>"
  },
  "Each loop": {
    "prefix": "each",
    "body": "<each loop=\"$1\">\n\t$2\n</each>"
  },
  "Include": {
    "prefix": "inc",
    "body": "<include src=\"$1\"></include>"
  },
  "Scope": {
    "prefix": "scp",
    "body": "<scope with=\"$1\">\n\t$2\n</scope>"
  },
  "Div with class": {
    "prefix": "d",
    "body": "<div class=\"$1\">\n\t$2\n</div>"
  },
  "Extends": {
    "prefix": "ext",
    "body": "<extends src=\"${1:base.html}\">\n\t$2\n</extends>"
  },
  "Block": {
    "prefix": "block",
    "body": "<block name=\"$1\">\n\t$2\n</block>"
  },
  "Lorem": {
    "prefix": "lorem",
    "body": "<lorem size=\"$1\"></lorem>"
  },
  "If condition": {
    "prefix": "if",
    "body": "<if condition=\"$1\"></if>"
  },
  "ElseIf condition": {
    "prefix": "elif",
    "body": "<elseif condition=\"$1\"></elseif>"
  },
  "Else condition": {
    "prefix": "else",
    "body": "<else>$1</else>"
  }
}
